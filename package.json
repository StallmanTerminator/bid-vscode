{
    "name": "bid-language",
    "displayName": "Brutal Interface Definition Language",
    "publisher": "brutal-org",
    "description": "Syntax support for the Brutal Interface Definition Language",
    "version": "0.0.3",
    "repository": "https://github.com/brutal-org/bid-vscode",
    "icon": "meta/icon.png",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bid",
                "aliases": [
                    "bid"
                ],
                "extensions": [
                    ".bid"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bid",
                "scopeName": "source.bid",
                "path": "./syntaxes/bid.tmLanguage.json"
            }
        ]
    }
}
